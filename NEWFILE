<?php

$count=0;
function isPrime($num){
	if($num==0 || $num==1)
		return false;
	$i=2;
	do{
		if($num==3 || $num==2)
			return true;
		if($num%$i==0)
			return false;
	$i++;
	}while($i<=$num/2);
	return true;
}

for($i=2;$i<=100;$i++){
if(isPrime($i)){
	echo "The number is  Prime\n".$i;
	$count++;
}

}
echo "Total\n".$count;

$str="A,B,C,D";

?>
